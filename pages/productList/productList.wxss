page {
  background-color: #fff;
}

.container {
  background-color: #fff;
  padding-bottom: 0;
}


.photo-scroll {
    width: 100%;
    height: 100%;
    padding-top: 10px;
    padding-left: 10px;
    box-sizing: border-box;

}

.photo-img {
    background-color: #eeeeee;
}

.photo-wrap {
    height: 375rpx;
    padding: 0px 10px 10px 0px;
    box-sizing: border-box;
    float: left;
}

.photo-wrap-5 {
    width: 50%;
}

.photo-wrap-3 {
    width: 33.3%;
}

.photo-wrap-7 {
    width: 66.7%;
}

.photo-img {
    height: 100%;
    width: 100%;
}